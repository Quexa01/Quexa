<%- include("partials/header"); -%>
<br>
<form class="" action="/upload" method="post" enctype="multipart/form-data">

    <% if (error) { %>
        <div class="alert alert-danger">
            <%= error %>
        </div>
    <% } %>

    <div class="form-group">
      <input class="form-control" type="text" name="courseCode" placeholder="Course Code">
    </div>
    <br>
    <div class="form-group">
      <input class="form-control" type="text" name="date" placeholder="Date">
    </div>
    <br>
    <div class="form-group">
      <input class="form-control" type="text" name="slot" placeholder="Slot">
    </div>
    <br>
    <div class="form-group">
        <input class="form-control" type="text" name="type" placeholder="Exam Type">
    </div>
    <br>
    <div class="form-group">
      <label>Images</label>
      <input class="form-control" type="file" name="image" multiple />
    </div>
    <br>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer"); -%>
